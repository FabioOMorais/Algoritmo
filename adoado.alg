algoritmo "semnome"

var
C, Q: inteiro
R, D: real
inicio
  C <- 1
  Escreva("Digite o valor de repetição: ")
  Leia(Q)
  Enquanto (C <= Q) faca
          Escreva("Qual o valor em R$?")
          Leia(R)
          D <- R/2.20
          Escreval(D:5:2)
          C <- C + 1
  FimEnquanto
fimalgoritmo
