algoritmo "Contador2.0"

var
contador, N, S: inteiro
inicio
      contador <- 1
      Enquanto (contador <= 5) faca
         Escreva("Digite um valor: ")
         Leia(N)
         S <- N + S
         contador <- contador + 1
      FimEnquanto
      Escreva("A soma de todos os números será: ", S)
fimalgoritmo
